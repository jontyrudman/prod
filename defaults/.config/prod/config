[options]
template_directory = ~/.local/share/prod
; Allows prod to run scripts in each
; template directory's root without confirmation
execute_scripts_noconfirm = false
; Allows prod to always delete scripts in each
; template directory's root without confirmation,
; once copied and possibly run
remove_scripts_noconfirm = false

[git]
; Don't set up a git repo by default
use_git = false
git_username =
; Don't use ssh by default
git_ssh = false
; git provider name.
; Supported:
; - gitlab
; - github
git_provider = gitlab
; git_access_token = 
; Attempt to make new repos private by default
git_private = true

